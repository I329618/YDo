<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd   http://www.springframework.org/schema/context         http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<alias alias="simpleSuggestionFacade" name="defaultSimpleSuggestionFacade" />
	<bean
		class="com.sap.sapbasket.facades.suggestion.impl.DefaultSimpleSuggestionFacade"
		id="defaultSimpleSuggestionFacade">
		<property name="simpleSuggestionService" ref="simpleSuggestionService" />
		<property name="userService" ref="userService" />
		<property name="categoryService" ref="categoryService" />
		<property name="productService" ref="productService" />
		<property name="productConverter" ref="carouselProductConverter" />
		<property name="cartService" ref="cartService" />
	</bean>

	<bean id="carouselProductConverter" parent="defaultProductConverter">
		<property name="populators">
			<list merge="true">
				<ref bean="productPricePopulator" />
			</list>
		</property>
	</bean>

	<alias alias="variantOptionDataConverter" name="acceleratorVariantOptionDataConverter" />
	<bean id="acceleratorVariantOptionDataConverter" parent="abstractPopulatingConverter">
		<lookup-method bean="variantOptionData" name="createTarget" />
		<property name="populators">
			<list>
				<ref bean="acceleratorVariantOptionDataPopulator" />
			</list>
		</property>
	</bean>

	<alias alias="acceleratorVariantOptionDataPopulator" name="defaultAcceleratorVariantOptionDataPopulator" />
	<bean
		class="com.sap.sapbasket.facades.populators.AcceleratorVariantOptionDataPopulator"
		id="defaultAcceleratorVariantOptionDataPopulator" parent="variantOptionDataPopulator">
		<property name="mediaService" ref="mediaService" />
		<property name="mediaContainerService" ref="mediaContainerService" />
		<property name="typeService" ref="typeService" />
		<property name="imageFormatMapping" ref="imageFormatMapping" />
		<property name="variantAttributeMapping">
			<map>
				<entry key="ApparelStyleVariantProduct.style" value="styleSwatch" />
			</map>
		</property>
	</bean>


	<alias alias="genderConverter" name="acceleratorGenderConverter" />
	<bean id="acceleratorGenderConverter" parent="abstractPopulatingConverter">
		<property name="targetClass"
			value="com.sap.sapbasket.facades.product.data.GenderData" />
		<property name="populators">
			<list>
				<ref bean="genderDataPopulator" />
			</list>
		</property>
	</bean>

	<alias alias="genderDataPopulator" name="defaultGenderDataPopulator" />
	<bean class="com.sap.sapbasket.facades.populators.GenderDataPopulator"
		id="defaultGenderDataPopulator">
		<property name="typeService" ref="typeService" />
	</bean>

	<bean class="com.sap.sapbasket.facades.populators.ApparelProductPopulator"
		id="apparelProductPopulator">
		<property name="genderConverter" ref="genderConverter" />
	</bean>

	<!-- Add the apparelProductPopulator to the productConverter -->
	<bean parent="modifyPopulatorList">
		<property name="list" ref="productConverter" />
		<property name="add" ref="apparelProductPopulator" />
	</bean>

	<!-- accelerator Image Format Mappings -->
	<alias alias="imageFormatMapping" name="acceleratorImageFormatMapping" />
	<bean id="acceleratorImageFormatMapping" parent="defaultImageFormatMapping">
		<property name="mapping">
			<map>
				<entry key="superZoom" value="1200Wx1200H" />
				<entry key="zoom" value="515Wx515H" />
				<entry key="store" value="365Wx246H" />
				<entry key="product" value="300Wx300H" />
				<!-- <entry key="thumbnail" value="96Wx96H"/> -->
				<entry key="thumbnail" value="300Wx300H" />
				<entry key="cartIcon" value="65Wx65H" />
				<entry key="styleSwatch" value="30Wx30H" />
			</map>
		</property>
	</bean>

	<alias alias="productPrimaryImagePopulator" name="acceleratorProductPrimaryImagePopulator" />
	<bean id="acceleratorProductPrimaryImagePopulator" parent="defaultProductPrimaryImagePopulator">
		<property name="imageFormats">
			<list>
				<value>zoom</value>
				<value>product</value>
				<value>thumbnail</value>
				<value>cartIcon</value>
			</list>
		</property>
	</bean>

	<alias alias="productGalleryImagesPopulator" name="acceleratorProductGalleryImagesPopulator" />
	<bean id="acceleratorProductGalleryImagesPopulator" parent="defaultProductGalleryImagesPopulator">
		<property name="imageFormats">
			<list>
				<value>superZoom</value>
				<value>zoom</value>
				<value>product</value>
				<value>thumbnail</value>
			</list>
		</property>
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.CustomerEmailContext"
		id="customerEmailContext" parent="abstractEmailContext" scope="prototype">
		<property name="customerConverter" ref="customerConverter" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.ForgottenPasswordEmailContext"
		id="forgottenPasswordEmailContext" parent="customerEmailContext"
		scope="prototype">
		<property name="customerConverter" ref="customerConverter" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.OrderNotificationEmailContext"
		id="orderNotificationEmailContext" parent="abstractEmailContext"
		scope="prototype">
		<property name="orderConverter" ref="orderConverter" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.DeliverySentEmailContext"
		id="deliverySentEmailContext" parent="abstractEmailContext" scope="prototype">
		<property name="consignmentConverter" ref="consignmentConverter" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.ReadyForPickupEmailContext"
		id="readyForPickupEmailContext" parent="abstractEmailContext" scope="prototype">
		<property name="consignmentConverter" ref="consignmentConverter" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.OrderCancelledEmailContext"
		id="orderCancelledEmailContext" parent="abstractEmailContext" scope="prototype">
		<property name="orderConverter" ref="orderConverter" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.OrderRefundEmailContext"
		id="orderRefundEmailContext" parent="abstractEmailContext" scope="prototype">
		<property name="orderConverter" ref="orderConverter" />
	</bean>
	<bean
		class="com.sap.sapbasket.facades.process.email.context.ConsignmentCollectionReminderEmailContext"
		id="consignmentCollectionReminderEmailContext" parent="abstractEmailContext"
		scope="prototype">
		<property name="consignmentConverter" ref="consignmentConverter" />
		<property name="formatFactory" ref="formatFactory" />
		<property name="customerServiceUncollectedConsignmentStrategy"
			ref="customerServiceUncollectedConsignmentStrategy" />
		<property name="orderConverter" ref="orderConverter" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.OrderPartiallyModifiedEmailContext"
		id="orderPartiallyModifiedEmailContext" parent="abstractEmailContext"
		scope="prototype">
		<property name="orderConverter" ref="orderConverter" />
		<property name="orderEntryConverter" ref="orderEntryConverter" />
		<property name="priceDataFactory" ref="priceDataFactory" />
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.OrderPartiallyCanceledEmailContext"
		id="orderPartiallyCanceledEmailContext" parent="orderPartiallyModifiedEmailContext"
		scope="prototype">
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.OrderPartiallyRefundedEmailContext"
		id="orderPartiallyRefundedEmailContext" parent="orderPartiallyModifiedEmailContext"
		scope="prototype">
	</bean>

	<bean
		class="com.sap.sapbasket.facades.process.email.context.NotPickedUpConsignmentCanceledEmailContext"
		id="notPickedUpConsignmentCanceledEmailContext" parent="abstractEmailContext"
		scope="prototype">
		<property name="consignmentConverter" ref="consignmentConverter" />
		<property name="priceDataFactory" ref="priceDataFactory" />
	</bean>

	<alias alias="searchPagePointOfServiceDistancePopulator"
		name="yacceleratorSearchPagePointOfServiceDistancePopulator" />
	<bean
		class="com.sap.sapbasket.facades.storefinder.populators.SearchPagePointOfServiceDistancePopulator"
		id="yacceleratorSearchPagePointOfServiceDistancePopulator">
		<property name="pointOfServiceDistanceConverter" ref="pointOfServiceDistanceConverter" />
	</bean>

	<bean class="com.sap.sapbasket.facades.product.data.GenderData"
		id="genderData" scope="prototype" />

	<alias alias="custCustomerFacade" name="defaultCustomCustomerFacade" />
	<bean
		class="com.sap.sapbasket.sapbasketfacades.impl.DefaultCustomCustomerFacade"
		id="defaultCustomCustomerFacade" parent="defaultCustomerFacade">
	</bean>

	<alias alias="customerPopulator" name="CustomCustomerPopulator" />
	<bean class="com.sap.sapbasket.facades.populators.CustomCustomerPopulator"
		id="CustomCustomerPopulator" parent="defaultCustomerPopulator" />

	<alias alias="TestAutomationFacade" name="defaultTestAutomationFacade" />
	<bean class="com.sap.sapbasket.facades.impl.DefaultAutomationFacade"
		id="defaultTestAutomationFacade" />




	<alias alias="crmkitFacade" name="defaultCRMKitFacade" />
	<bean class="com.sap.sapbasket.facades.impl.DefaultCRMKitFacade"
		id="defaultCRMKitFacade" />
	<alias alias="crmkitConverter" name="defaultCRMKitConverter" />
	<bean id="defaultCRMKitConverter" parent="abstractPopulatingConverter">
		<property name="targetClass"
			value="de.hybris.platform.commercefacades.automation.data.CRMKitData" />
		<property name="populators">
			<list merge="true">
				<ref bean="crmkitPopulator" />
			</list>
		</property>
	</bean>
	<alias alias="crmkitPopulator" name="defaultCRMKitPopulator" />
	<bean class="com.sap.sapbasket.facades.populators.CustomCRMKitPopulator"
		id="defaultCRMKitPopulator">
		<property name="kitOwnerConverter" ref="b2BCustomerConverter" />
		<property name="b2bUnitConverter" ref="b2bUnitConverter" />
	</bean>
	
	<alias alias="crmFacade" name="defaultCRMFacade" />
	<bean class="com.sap.sapbasket.facades.impl.DefaultCRMFacade"
		id="defaultCRMFacade" />
	<alias alias="crmConverter" name="defaultCRMConverter" />
	<bean id="defaultCRMConverter" parent="abstractPopulatingConverter">
		<property name="targetClass"
			value="de.hybris.platform.commercefacades.automation.data.CRMData" />
		<property name="populators">
			<list merge="true">
				<ref bean="crmPopulator" />
			</list>
		</property>
	</bean>
	
	<alias alias="crmPopulator" name="defaultCRMPopulator" />
	<bean class="com.sap.sapbasket.facades.populators.CustomCRMPopulator"
		id="defaultCRMPopulator">
		<property name="kitOwnerConverter" ref="b2BCustomerConverter" />
	</bean>

	
</beans>